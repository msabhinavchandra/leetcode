class Solution(object):
    def isPalindrome(self, s):
        lf=0
        rt=len(s)-1
        while lf<rt:
            while lf<rt and not s[lf].isalnum():
                lf=lf+1
            while lf<rt and not s[rt].isalnum():
                rt=rt-1
            if s[lf].lower()==s[rt].lower():
                lf=lf+1
                rt=rt-1
            else:
                return False
        return True

        """
        :type s: str
        :rtype: bool
        """
        
        #alternate
        
class Solution(object):
    def isPalindrome(self, s):
        # i=0
        tp=''
        for  i in  s:
            if i.isalnum():
                tp=tp+i.lower()
            # i=i+1
        if tp==tp[::-1]:
            return True
        else:
            return False



        """
        :type s: str
        :rtype: bool
        """
        
#alternate way of checking the alphanumeric with the help of ascii
        def isalphanum(self,n):
            return (ord('A')<=ord(n)<=ord('Z') or
                    ord('a')<=ord(n)<=ord('z') or
                    ord('0')<=ord(n)<=ord('9'))
