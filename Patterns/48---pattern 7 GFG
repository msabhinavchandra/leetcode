class Solution:
    def printTriangle(self, N):
        for i in range(N):
            for j in range(i+1,N):
                print(" ",end="")
            for h in range(i+1):
            # if j>=4:
                print("*",end="")
            for g in range(i):
                print("*",end='')
            print("")
        # Code here
