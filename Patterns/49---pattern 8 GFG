#User function Template for python3

class Solution:
    def printTriangle(self, N):
        for i in range(N):
    # print("")
            for j in range(i):
                print(' ',end="")
            for h in range(2*N-2*i-1):
                print("*",end="")
            for j in range(i):
                print(' ',end="")
            print('')
